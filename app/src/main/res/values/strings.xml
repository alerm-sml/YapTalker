<resources xmlns:tools="http://schemas.android.com/tools" xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
  <string name="app_name">YapTalker</string>

  <!-- NAVIGATION DRAWER -->
  <!-- Header items -->
  <string name="nav_drawer_guest_name">Guest</string>
  <!-- Navigation items -->
  <string name="nav_drawer_main_page">Main page</string>
  <string name="nav_drawer_forums">Forums</string>
  <string name="nav_drawer_active_topics">Active topics</string>
