  <string name="nav_drawer_sign_in">Sign In</string>
  <string name="nav_drawer_sign_out">Sign Out</string>

  <!-- NEWS -->
  <plurals name="news_comments_template">
    <item quantity="one"><xliff:g id="icon">{cmd-comment-multiple-outline}</xliff:g> <xliff:g id="count">%s</xliff:g> comment</item>
    <item quantity="few"><xliff:g id="icon">{cmd-comment-multiple-outline}</xliff:g> <xliff:g id="count">%s</xliff:g> comments</item>
    <item quantity="many"><xliff:g id="icon">{cmd-comment-multiple-outline}</xliff:g> <xliff:g id="count">%s</xliff:g> comments</item>
    <item quantity="other"><xliff:g id="icon">{cmd-comment-multiple-outline}</xliff:g> <xliff:g id="count">%s</xliff:g> comments</item>
  </plurals>

  <string name="news_forum_title_template" translatable="false"><xliff:g id="bullet">\u25AA</xliff:g> <xliff:g id="forum">%s</xliff:g></string>

  <!-- FORUMS -->
  <string name="forum_comments_template">Comments: <xliff:g id="answers">%s</xliff:g></string>
  <string name="forum_pinned_topic_template" translatable="false"><xliff:g id="icon">{cmd-pin}</xliff:g> <xliff:g id="title">%s</xliff:g></string>
  <string name="forum_closed_topic_template" translatable="false"><xliff:g id="icon">{cmd-lock}</xliff:g> <xliff:g id="title">%s</xliff:g></string>
  <string name="forum_pinned_and_closed_topic_template" translatable="false"><xliff:g id="icons">{cmd-lock} {cmd-pin}</xliff:g> <xliff:g id="title">%s</xliff:g></string>

  <!-- NAVIGATION PANEL -->
  <string name="navigation_pages_template"><xliff:g id="page">%d</xliff:g> of <xliff:g id="total_pages">%d</xliff:g></string>
  <string name="navigation_go_to_page_title">Go to page</string>
  <string name="navigation_go_to_page_hint">Enter page number…</string>
  <string name="navigation_page_not_available">Page <xliff:g id="page">%d</xliff:g> is not available</string>
  <string name="navigation_previous" translatable="false" tools:keep="@string/navigation_previous">&lt;</string>
  <string name="navigation_next" translatable="false" tools:keep="@string/navigation_next">&gt;</string>
  <string name="navigation_first" translatable="false" tools:keep="@string/navigation_first">&lt;&lt;</string>
  <string name="navigation_last" translatable="false" tools:keep="@string/navigation_last">&gt;&gt;</string>

